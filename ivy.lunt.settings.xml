<ivysettings>
  <resolvers>
    <url name="spiralcraft">
      <ivy 
        pattern="http://publish.spiralcraft.com/[module]/current/[revision]/artifacts/ivy.xml"
      />
      <artifact pattern="http://publish.spiralcraft.com/[module]/current/[revision]/artifacts/[module].[ext]" 
      />
    </url>
  
    <filesystem name="luntlocal">
      <ivy 
        pattern="/opt/luntbuild/publish/[module]/current/[revision]/artifacts/ivy.xml"
      />
      <artifact pattern="/opt/luntbuild/publish/[module]/current/[revision]/artifacts/[module].[ext]" 
      />
    </filesystem>
  </resolvers>
  <settings defaultResolver="spiralcraft"/>
  <modules>
    <module organisation="spiralcraft" name=".*" resolver="spiralcraft" />
  </modules>
</ivysettings>